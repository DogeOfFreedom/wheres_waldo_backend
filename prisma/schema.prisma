generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Player {
  id        String  @id @default(uuid())
  rank      Int     @unique
  name      String
  time      String
  anonymous Boolean
}

model Level {
  id     String @id @default(uuid())
  name   String @unique
  img    String @unique
  coords Json
}
